{
    "aliasColors": {},
    "bars": false,
    "dashLength": 10,
    "dashes": false,
    "datasource": null,
    "fieldConfig": {
    "defaults": {
        "custom": {}
    },
    "overrides": []
    },
    "fill": 1,
    "fillGradient": 0,
    "gridPos": {
    "h": 9,
    "w": 12,
    "x": 0,
    "y": 0
    },
    "hiddenSeries": false,
    "id": {{ data.id }},
    "legend": {
    "alignAsTable": true,
    "avg": true,
    "current": true,
    "max": true,
    "min": true,
    "show": true,
    "sort": "current",
    "sortDesc": true,
    "total": false,
    "values": true
    },
    "lines": true,
    "linewidth": 1,
    "nullPointMode": "connected",
    "options": {
    "dataLinks": []
    },
    "percentage": false,
    "pointradius": 2,
    "points": false,
    "renderer": "flot",
    "seriesOverrides": [],
    "spaceLength": 10,
    "stack": false,
    "steppedLine": false,
    "targets": [
    {
        "alias": "Hostname: $tag_hostname | Interface: $tag_ifDescription | $col",
        "groupBy": [
        {
            "params": [
            "$__interval"
            ],
            "type": "time"
        },
        {
            "params": [
            "null"
            ],
            "type": "fill"
        }
        ],
        "orderByTime": "ASC",
        "policy": "default",
        "query": "SELECT non_negative_derivative(\"ifHCInOctets\", 1m) /8 AS \"Input\", non_negative_derivative(\"ifHCOutOctets\", 1m) /8 AS \"Output\" FROM \"interface\" WHERE (\"hostname\" = '{{ data.hostname }}' AND \"ifDescription\" = '{{ data.interface }}') AND $timeFilter GROUP BY \"ifDescription\", \"ifAlias\", \"agent_host\",\"hostname\"\n",
        "rawQuery": true,
        "refId": "A",
        "resultFormat": "time_series",
        "select": [
        [
            {
            "params": [
                "value"
            ],
            "type": "field"
            },
            {
            "params": [],
            "type": "mean"
            }
        ]
        ],
        "tags": [
        {
            "key": "hostname",
            "operator": "=",
            "value": "{{ data.hostname }}"
        }
        ]
    }
    ],
    "thresholds": [],
    "timeFrom": null,
    "timeRegions": [],
    "timeShift": null,
    "title": "{{ data.hostname }} | {{ data.interface }}",
    "tooltip": {
    "shared": true,
    "sort": 2,
    "value_type": "individual"
    },
    "type": "graph",
    "xaxis": {
    "buckets": null,
    "mode": "time",
    "name": null,
    "show": true,
    "values": []
    },
    "yaxes": [
    {
        "format": "bps",
        "label": null,
        "logBase": 1,
        "max": null,
        "min": null,
        "show": true
    },
    {
        "format": "short",
        "label": null,
        "logBase": 1,
        "max": null,
        "min": null,
        "show": true
    }
    ],
    "yaxis": {
    "align": false,
    "alignLevel": null
    }
}
